{{yield}}

{{#if pagination}}
  {{ list-pagination currentPage=currentPage pageSize=pageSize numItems=instances.length changed=(action "setPage")}}
{{/if}}

<ul class="entryList">
    {{#each (limit-array currentPageItems limit) as |instance|}}
      {{#if attributes}}
        {{entry-list-item instance=instance attributes=attributes}}
      {{else}}
        {{entry-list-item instance=instance}}
      {{/if}}
    {{/each}}
</ul>
