{{outlet}}

<div class="row">
  <div class="col s12 m12">
    <div class="mainCard">
      <div class="mainCard__header mainCard__headerLight">
        <h2 class="mainCard__headerTitle"> {{titleCached}}</h2>
        {{#link-to "protected.editorga" model.id class="mainCard__headerAction"}}<i class="material-icons">mode_edit</i>{{/link-to}}
      </div>
      <div>
        {{entry-detail model=model}}
        <h2>Übergeordnete Orga:</h2>
        {{#if model.parentOrga}}
         <ul class="entryList">
            {{entry-list-item instance=model.parentOrga}}
          </ul>
        {{else}}
          Es exisitert keine übergeordnete Orga
        {{/if}}
        <h2>Untergeordnete Orgas:</h2>
        {{#if model.subOrgas}}
          {{entry-list instances=model.subOrgas}}
        {{else}}
          Es existieren keine Orgas
        {{/if}}
      </div>
    </div>
  </div>
</div>
