{
  "name": "backend-ui",
  "version": "1.0.0",
  "description": "afeefa backend ui",
  "author": "Jens Struwe <jens@struwe.net>",
  "private": true,
  "scripts": {
    "debug": "webpack --config=build/webpack.dev.conf --progress --colors --profile --json > stats.json",
    "dev": "npm run dll && npm run dev-no-dll",
    "dev-no-dll": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dll": "webpack --config=build/webpack.dev.dll.conf --progress --colors --profile --json > stats.json",
    "build-staging": "IS_STAGING=true node build/build.js",
    "build": "IS_STAGING=false node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --no-single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "lint:css": "stylelint 'src/**/*.vue' 'src/**/*.scss'"
  },
